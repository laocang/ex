body {
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	background: #FFFFFF;
	-webkit-touch-callout: none;
}

body,table,tr,td,a {
	color: #000000;
}

html {
	height: 100%;
	font-family: SimSun;
	color: #818181;
}


textarea,select {
	border: 1px solid #b7b7b7;
	font-size: 1em;
}

a {
	text-decoration: none;
	color: #000000;
}

select {
	height: 1.4em;
}

button {
	width: 4em;
	line-height: 1.4em;
	border-width: 0;
	font-size: 1.2rem;
	font-weight: bold;
	letter-spacing: .3em;
	text-indent: .5em;
	border-radius: 5px;
	text-align: center;
	cursor: pointer;
	color: #000;
	background-color: #fff;
}

.hide {
	display: none;
}

.ul {
	padding: 0;
	margin: 0;
	list-style-type: none;
}

.ui-pinch {
	padding: 1.2em 1.2em 0.1em 1.2em;
}

.ui-select-show {
	display: none;
}

.ui-nav>a,.ui-tab>a {
	text-decoration: none;
	color: #000;
}

.ui-tab-nav {
	width: 100%;
	font-size: 1.38em;
	display: table;
}

.ui-tab-nav>a {
	width: 33%;
	padding: .8em 0;
	display: table-cell;
	position: relative;
	outline: none;
	text-align: center;
	color: #fff;
	background-color: #a8a8a8;
}

.ui-tab-nav>.current:after {
	width: 100%;
	height: 0;
	position: absolute;
	bottom: 0;
	left: 0;
	content: "";
	border-top: 3px solid #6bc30d;
}

.ui-article {
	padding-top: 0;
}

.ui-article>h2 {
	padding: .5em 0;
	margin: 0;
	font-size: 1.39em;
	text-align: center;
	color: #888;
}

.ui-article>p {
	padding: .8em 0;
	margin: 0;
	border-bottom: 1px solid #d8d8d8;
}

.ui-forms-footer {
	background-color: #fff;
}

.ui-forms-footer>div {
	padding: 0.1em 0
}

.ui-table-div {
	width: 100%;
	display: table;
}

.ui-table-div>div {
	display: table-cell;
	vertical-align: middle;
	color: #878787;
}

.ui-table-div>div:first-child {
	width: 5em;
	padding-right: .5em;
	white-space: nowrap;
}

.ui-table-div .ui-select {
	width: 100%;
	color: #878787;
	background-color: #ebebeb;
}

.ui-opinion {
	width: 100%;
	height: 100px;
	margin: 0;
	box-sizing: border-box;
	resize: none;
}

div.ui-forms-butpanel {
	padding-bottom: 0;
	font-size: 0;
	text-align: justify;
}

.ui-forms-butpanel:after {
	width: 90%;
	height: 1px;
	line-height: 0;
	display: inline-block;
	content: "";
	overflow: hidden;
}

.ui-data-tab>a {
	width: 50%;
}

.ui-data-tab>.current {
	color: inherit;
	background-color: transparent;
}

.ui-data-tab>.current:after {
	content: normal;
}

.ui-data {
	border: 1px solid #dbdddf;
	border-radius: 5px;
	overflow: hidden;
}

.ui-data>li:first-child {
	margin-top: 0;
}

.ui-data>li {
	margin-top: .3em;
}

.ui-data>li>p {
	padding: 1em;
	margin: 0;
	font-size: 16px;
}

.ui-data-time {
	color: #666;
	background-color: #dbdddf;
}

.ui-data-info:after {
	
}

.ui-data .ui-data-info {
	padding-right: 0.5em;
	background-color: #fff;
}

.ui-sche-div {
	text-align: center;
	padding-top: 0.8em;
	margin-top: 0.5em;
	width: 100%;
	height: 45px;
	line-height: 45px;
	background: #ebebeb;
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 11;
	_position: absolute;
	_top: expression(documentElement.scrollTop +
		documentElement.clientHeight-this.offsetHeight);
}

.ui-sche-div>button {
	width: 5em;
	letter-spacing: .15em;
	text-indent: 0.35em;
	font-size: 1.1em;
}

.ui-sche-div>button:first-child {
	float: left;
	margin-left: 1em;
}

.ui-sche-div>button:last-child {
	float: right;
	margin-right: 1em;
}

.ui-che-div {
	text-align: center;
	padding-top: 0.8em;
	margin-top: 0.5em;
	width: 100%;
	height: 45px;
	line-height: 45px;
	background: #ebebeb;
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 11;
	_position: absolute;
	_top: expression(documentElement.scrollTop +
		documentElement.clientHeight-this.offsetHeight);
}

.ui-che-div>table>tbody>tr>td>button {
	width: 5em;
	letter-spacing: .15rem;
	text-indent: 0.25rem;
	font-size: 1.1rem;
	line-height: 2rem;
}