/**********contentFrame************/
.divcontentFrame {
	padding: 10px;
}

.divSelectList {
	padding: 5px 5px 5px 20px;
	border-bottom: 2px dotted #ADBDC5;
}

.divSelectList .SelectOption {
	width: 150px;
}

.divContentList {
	overflow-y: auto;
}

.divContentList ul,.divContentList ul li {
	padding: 0px;
	margin: 0px;
	list-style: none;
	cursor: pointer;
}

.divContentList ul {
	padding: 5px 5px 5px 20px;
	border-bottom: 2px dotted #ADBDC5;
	margin-right: 10px;
}

.areaEdit {
	background: url(../images/pda/edit.jpg) no-repeat left center;
	display: inline-block;
	width: 13px;
	height: 13px;
	cursor: pointer;
}

.spanforArea,.spanforTime {
	font-size: 18px;
	font-family: "宋体";
	color: #393939;
	font-weight: 500;
}

.ContentTitle {
	text-indent: 26px;
	padding-top: 5px ! important;
	font-size: 18px;
	font-family: "宋体";
	color: Black;
	font-weight: 500;
}

.sidebar a:link,.sidebar a:visited {
	color: #FF0000;
	text-decoration: none;
}

.sidebar a:hover,.sidebar a:active {
	color: #000000;
	text-decoration: underline;
}

.appsidebar a:link,.appsidebar a:visited {
	color: #000000;
	text-decoration: none;
}

.appsidebar a:hover,.appsidebar a:active {
	color: #000000;
	text-decoration: underline;
}