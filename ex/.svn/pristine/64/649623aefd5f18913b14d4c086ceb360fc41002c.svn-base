}

.divMenuContent {
	padding: 13px 45px 0px 77px;
}

.bigFonts {
	font-size: 18px;
	font-family: "宋体";
	text-align: left;
	height: 20px;
	line-height: 22px;
	color: Black;
	font-weight: bold;
}

.smallFonts {
	font-size: 16px;
	font-family: "宋体";
	text-align: left;
	height: 20px;
	line-height: 22px;
	color: #3E484F;
	font-weight: normal;
	position: relative;
	left: 2px;
}

.FenGeXian {
	width: 6px;
	background: url(../images/pda/fengexian.jpg) repeat-y left top;
	position: absolute;
	right: 0px;
	top: 58px;
	height: 100%;
}
/******TopFrame******/
.backButton {
	width: 65px;
	height: 37px;
	position: absolute;
	top: 10px;
	left: 60px;
}

.backButton a {
	width: 65px;
	height: 37px;
	background: url(../images/pda/backButton.png) no-repeat left top;
	cursor: pointer;
	text-decoration: none;
	display: block;
}

.forwardButton {
	width: 65px;
	height: 37px;
	position: absolute;
	top: 10px;
	right: 60px;
}

.forwardButton a {
	width: 65px;
	height: 37px;
	background: url(../images/pda/forwardButton.png) no-repeat left top;
	cursor: pointer;
	text-decoration: none;
	display: block;
}

