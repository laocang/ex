body {
	padding: 0px;
	margin: 0px;
	font-size: 12px;
	font-family: "宋体";
}

a {
	text-decoration: none;
	padding: 0px;
	margin: 0px;
}
/***左菜单头部***/
.divMenuHead {
	height: 58px;
	background: url(../images/pda/divMenuHead.jpg) repeat-x left top;
	position: relative;
	width: 300px;
}

.divMenuHeadTopFrame {
	width: 100%;
}

.divMenuHeadLeft {
	float: left;
	background: url(../images/pda/divMenuHeadLeft.jpg) no-repeat left top;
	width: 9px;
	height: 58px;
}

.divMenuHeadRight {
	float: right;
	background: url(../images/pda/divMenuHeadRight.jpg) no-repeat -3px top;
	width: 9px;
	height: 58px;
}

.divMenuHeadRightIcon {
	position: absolute;
	width: 3px;
	height: 58px;
	background: url(../images/pda/divMenuHeadLeft.jpg) no-repeat left top;
	right: 0px;
	top: 0px;
}

.divMenuHeadLeft1 {
	float: left;
	background: url(../images/pda/divMenuHeadLeft.jpg) no-repeat -3px top;
	width: 9px;
	height: 58px;
}

.divMenuHeadRight1 {
	float: right;
	background: url(../images/pda/divMenuHeadRight.jpg) no-repeat right top;
	width: 9px;
	height: 58px;
}

.divMenuHeadCont {
	height: 34px;
	width: 121px;
	background: url(../images/pda/divMenuHeadCont.png) no-repeat left top;
	position: absolute;
	left: 50%;
	margin-left: -60px;
	top: 12px;
}
/***左菜单内容***/
.leftMenu {
	position: relative;
	width: 298px;
	overflow-y: auto;
	overflow-x: hidden;
}

a.divMenu {
	height: 76px;
	display: block;
	background: url(../images/pda/divMenuBG.jpg) repeat-x left top;
	position: relative;
	cursor: pointer;
	width: 282px;
}

a:hover.divMenu,a.divMenuSL {
	background: url(../images/pda/divMenuBGSL.jpg) repeat-x left top;
	height: 76px;
	display: block;
	position: relative;
	cursor: pointer;
	width: 282px;
}

.divMenuImg {
	position: absolute;
	left: 3px;
	top: 4px;
	background: url(../images/pda/icon1.png) no-repeat left top;
	width: 70px;
	height: 72px;
}

